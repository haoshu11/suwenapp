.container {
    width: 100%;
    height: 100vh;
    position: relative;
}

.container .page-section-title {
    position: fixed;
    top: 15rpx;
    left: 15rpx;
}

.container .box-is-visible-tip {
    position: fixed;
    top: 50%;
    width: 100%;
    text-align: center;
    font-size: 14px;
    color: #000;
}

#target {
    margin: 0px auto 0;
    width: 150px;
    height: 150px;
    background: lightblue;
}