.map-context-list {
    background-color: #fff;
    padding: 0 15rpx;
}

.map-context-list .title {
    font-weight: 600;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    box-sizing: border-box;
}

.map-context-list .item {
    height: 60rpx;
    line-height: 60rpx;
    box-sizing: border-box;
}

.map-context-list .item:last-child {
    border: 0;
}